{
  "groupName ": "all",
  "branchPrefix": "renovate/reno-",
  "dryRun": false,
  "gitAuthor": "Renovate Bot <bot@renovateapp.com>",
  "labels": [
    "renovate-dependencies"
  ],
  "onboarding": true,
  "onboardingConfigFileName": ".github/renovate.json",
  "onboardingBranch": "renovate/reno-cfg",
  "platform": "github",
  "hashedBranchLength": 20,
  "github-actions": {
    "enabled": false
  },
  "docker": {
    "enabled": false
  },
  "repositories": [
	"khansaofx/testvulnerabilities",
  ],  
  "extends": ["config:base", ":disableRateLimiting"],
   "packageRules": [
    {
      "matchManagers": ["npm"],
      "matchPackagePatterns": ["*"],
      "groupName": "everything",
      "enabled": false 
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
}
